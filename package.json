{
  "name": "@escapace/typelevel",
  "description": "typelevel TypeScript library",
  "version": "0.0.0",
  "author": "escapace <opensource@escapace.com>",
  "bugs": "https://github.com/escapace/typelevel/issues",
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/node": "14.0.6",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "chai": "4.2.0",
    "cross-env": "7.0.2",
    "dtslint": "3.6.9",
    "eslint": "6.8.0",
    "eslint-config-escapace": "3.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard-with-typescript": "16.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "25.4.2",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.5",
    "lint-staged": "10.2.7",
    "mocha": "7.2.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "semantic-release": "17.0.8",
    "syncpack": "5.0.1",
    "typescript": "3.9.3"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "files": [
    "hkt.d.ts",
    "index.d.ts",
    "typelevel"
  ],
  "homepage": "https://github.com/escapace/typelevel",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run typecheck && npm run test"
    }
  },
  "keywords": [
    "TypeScript",
    "type-level",
    "typelevel"
  ],
  "license": "MPL-2.0",
  "lint-staged": {
    "package.json": [
      "syncpack-format --source",
      "syncpack-set-semver-ranges --dev --source"
    ],
    "{src,test}/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "{src,test}/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": "escapace/typelevel",
  "scripts": {
    "build": "rimraf index.d.ts hkt.d.ts typelevel && npx recce build -e src/index.ts -m esm && rimraf lib/cjs lib/esm lib/umd && mv lib/types/* .",
    "lint": "eslint 'src/**/*.ts'",
    "test": "dtslint --expectOnly --localTs node_modules/typescript/lib test",
    "typecheck": "tsc -p test/tsconfig.json --noEmit"
  },
  "sideEffects": false,
  "typings": "index.d.ts"
}
